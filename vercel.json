{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*\\.(?:png|jpg|jpeg|gif|svg|css|js))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
